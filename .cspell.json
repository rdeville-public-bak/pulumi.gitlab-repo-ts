{
  "version": "0.1",
  "gitignoreRoot": ".",
  "useGitignore": true,
  "dictionaries": [
    "css",
    "html",
    "fonts",
    "typescript",
    "softwareTerms",
    "companies",
    "lorem-ipsum",
    "project-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": ".cspell/words.txt",
      "noSuggest": true
    }
  ],
  "ignorePaths": [
    "CHANGELOG.md",
    "package.json",
    "yarn.lock",
    "Pulumi.*.yaml",
    "**/snapshots/*.snap",
    "**/snapshots/*.md"
  ],
  "ignoreRegExpList": [
    "Email",
    "Urls",
    "#[\\w-]*"
  ]
}
